@model IEnumerable<ConsommiTounsi.Domaine.Entities.Product.Marque>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nameMarque)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descriptionMarque)
        </th>
        <th></th>
    </tr>

@foreach (var item in ViewBag.brands) {
    <tr>
        <td>
            @item.nameMarque
        </td>
        <td>
            @item.descriptionMarque
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MarqueId }) |
            @Html.ActionLink("Details", "Details", new { id=item.MarqueId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MarqueId })
        </td>
    </tr>
}

</table>
